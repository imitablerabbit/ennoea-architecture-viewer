[
    {
        "type": "variable",
        "regexes": [
            {
                "regexString": "\\.([a-zA-Z_][\\w-]*)\\b",
                "captureGroup": 1
            }
        ]
    },
    {
        "type": "number",
        "regexes": [
            "\\b\\d*\\.?\\d+\\b"
        ]
    },
    {
        "type": "function",
        "regexes": [
            "\\b[a-zA-Z_][a-zA-Z0-9_-]+?(?=\\()"
        ]
    },
    {
        "type": "import",
        "regexes": [
            {
                "regexString": "(?:from)\\s+([\\w\\.]+)",
                "captureGroup": 1
            },
            {
                "regexString": "(?:import)\\s+([\\w, \\.]+)",
                "captureGroup": 1
            }
        ]
    },
    {
        "type": "keyword",
        "regexes": [
            "\\band\\b",
            "\\bas\\b",
            "\\bassert\\b",
            "\\bbreak\\b",
            "\\bclass\\b",
            "\\bcontinue\\b",
            "\\bdef\\b",
            "\\belif\\b",
            "\\belse\\b",
            "\\bexcept\\b",
            "\\bexec\\b",
            "\\bfinally\\b",
            "\\bfor\\b",
            "\\bfrom\\b",
            "\\bglobal\\b",
            "\\bif\\b",
            "\\bimport\\b",
            "\\bin\\b",
            "\\bis\\b",
            "\\blambda\\b",
            "\\bnot\\b",
            "\\bor\\b",
            "\\bpass\\b",
            "\\bprint\\b",
            "\\braise\\b",
            "\\breturn\\b",
            "\\btry\\b",
            "\\bwhile\\b",
            "\\bwith\\b",
            "\\byield\\b"
        ]
    },
    {
        "type": "literal-value",
        "regexes": [
            "\\bTrue\\b",
            "\\bFalse\\b",
            "\\bNone\\b"
        ]
    },
    {
        "type": "comment",
        "regexes": [
            "#.+",
            "\\/\\*[\\S\\s]*?\\*\\/"
        ]
    },
    {
        "type": "wrapping",
        "regexes": [
            "\"(\\\\.|[^\"])*\"",
            "'.?'"
        ],
        "precedence": "comment"
    }
]
